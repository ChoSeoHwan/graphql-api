{
  "name": "graphql-api",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "eslint": "eslint \"./src/**/*.ts\"",
    "eslint:fix": "yarn eslint --fix",
    "lint-staged": "lint-staged",
    "dev": "cross-env NODE_ENV=development nodemon --config .config/nodemon.json src/index.ts",
    "build": "rollup -c rollup.config.js",
    "prestart": "yarn build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "prestart:dev": "yarn build",
    "start:dev": "cross-env NODE_ENV=development node dist/index.js"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/runtime": "^7.16.5",
    "@choseohwan/babel-preset-base": "^1.0.1",
    "@choseohwan/eslint-config-base": "^1.0.14",
    "@choseohwan/prettier-config-base": "^1.0.7",
    "@choseohwan/rollup-builder": "^1.1.7",
    "@choseohwan/rollup-plugin-builder-base": "^1.1.1",
    "@choseohwan/tsconfig-base": "^1.0.12",
    "@commitlint/cli": "^16.0.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@types/bluebird": "^3.5.36",
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rollup": "^2.62.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-node": "^10.4.0",
    "ts-toolbelt": "^9.6.0",
    "tsconfig-paths": "^3.12.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.4",
    "typescript-transform-paths": "^3.3.1"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "dotenv": "^10.0.0"
  }
}
